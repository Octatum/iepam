{
  "name": "iepam-serverless-functions",
  "description": "Plataforma conjunta de IEPAM",
  "version": "0.1.0@prealpha",
  "author": "Octatum <contacto@octatum.com>",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "core-js": "^2.6.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:gatsby": "gatsby build",
    "build:lambda": "tsc && netlify-lambda -c webpack-config/webpack.functions.js build functions",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:gatsby && npm run build:lambda",
    "watch:functions": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run firebase:functions\"",
    "watch-ts": "tsc -w",
    "firebase:functions": "nodemon --watch ./functions --exec \"firebase serve --only functions\"",
    "watch-built-files": "nodemon functions/server.js",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "netlify-lambda": "^1.4.3",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
