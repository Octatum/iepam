{
  "name": "iepam-serverless-functions",
  "description": "Serverless functions for iepam",
  "version": "0.1.0",
  "author": "Octatum <contacto@octatum.com>",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "core-js": "^2.6.5",
    "errorhandler": "^1.5.0",
    "express-validator": "^5.3.1",
    "firebase": "^5.9.0",
    "firebase-admin": "^7.1.0",
    "firebase-functions": "^2.2.0",
    "firebaseui": "^3.5.2",
    "lusca": "^1.6.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "prop-types": "^15.7.2",
    "serverless-http": "^1.9.1",
    "slick-carousel": "^1.8.1",
    "winston": "^3.2.1"
  },
  "keywords": [
    "firebase",
    "serverless",
    "firestore"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc && netlify-lambda -c webpack-config/webpack.functions.js build functions",
    "develop": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run firebase:functions\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-ts": "tsc -w",
    "firebase:functions": "nodemon --watch ./functions --exec \"firebase serve --only functions\"",
    "watch-built-files": "nodemon functions/server.js"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.119",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.41",
    "@types/node-fetch": "^2.1.4",
    "@types/nodemailer": "^4.6.5",
    "@types/passport": "^0.4.7",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "netlify-lambda": "^1.4.3",
    "nodemon": "^1.18.10",
    "shelljs": "^0.8.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
