{
  "connection": "default",
  "collectionName": "userdata",
  "info": {
    "name": "userdata",
    "description": "datos que tienen los usuarios"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "nombre completo": {
      "default": "",
      "type": "string"
    },
    "genero": {
      "default": "",
      "type": "string"
    },
    "fecha de nacimiento": {
      "default": "",
      "type": "date"
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "ciudad": {
      "default": "",
      "type": "string"
    },
    "estado": {
      "default": "",
      "type": "string"
    }
  }
}